{
  "name": "grommet-icons",
  "version": "1.1.0",
  "main": "index.js",
  "module": "es6/index.js",
  "jsnext:main": "es6/index.js",
  "description": "iconography for grommet apps.",
  "authors": [
    "Alan Souza",
    "Bryan Jacquot",
    "Chris Carlozzi",
    "Eric Soderberg"
  ],
  "homepage": "http://grommet.io/grommet-icons/",
  "bugs": "https://github.com/grommet/grommet-icons/issues",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/grommet/grommet-icons.git"
  },
  "scripts": {
    "build": "webpack --mode production && babel ./src/js/ --ignore '__tests__' --out-dir ./dist && cross-env BABEL_ENV=es6 babel ./src/js/ --ignore '__tests__' --out-dir ./dist/es6",
    "generate-icons": "babel-node ./tools/generate-icons",
    "generate-markdown": "babel-node ./tools/generate-markdown",
    "release-stable": "babel-node ./tools/release-stable",
    "lint": "eslint src",
    "storybook": "start-storybook -p 9002 -c storybook",
    "test": "jest",
    "test-update": "jest --updateSnapshot",
    "test-watch": "jest --watchAll"
  },
  "peerDependencies": {
    "react": ">= 16.3.2",
    "react-dom": ">= 16.3.2",
    "styled-components": ">= 3.2.6"
  },
  "devDependencies": {
    "@storybook/addon-knobs": "^3.4.8",
    "@storybook/react": "^3.4.8",
    "babel-cli": "^6.22.2",
    "babel-core": "^6.5.2",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^23.0.1",
    "babel-loader": "^7.1.1",
    "babel-plugin-styled-components": "^1.5.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-1": "^6.24.1",
    "camel-case": "^3.0.0",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^5.2.0",
    "del": "^3.0.0",
    "eslint": "^5.2.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-parallel": "^0.3.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.9.1",
    "fs-extra": "^7.0.0",
    "jest-cli": "^23.4.1",
    "jest-styled-components": "5.0.1",
    "json-loader": "^0.5.4",
    "pascal-case": "^2.0.1",
    "pre-commit": "^1.2.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-test-renderer": "^16.4.1",
    "simple-git": "^1.96.0",
    "styled-components": "^3.3.2",
    "webpack": "^4.16.2",
    "webpack-cli": "^3.1.0",
    "xml2js": "^0.4.19",
    "xmlbuilder": "^10.0.0"
  },
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "lcov"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js}"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](dist|node_modules)[/\\\\]"
    ]
  },
  "pre-commit": [
    "lint",
    "test"
  ]
}
